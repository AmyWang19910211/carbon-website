{
  "name": "gatsby",
  "build": {
    "dockerfile": "Dockerfile",
    "args": {
      "VARIANT": "14"
    }
  },
  "settings": {
    "terminal.integrated.shell.linux": "/bin/bash"
  },
  "extensions": [
    "dbaeumer.vscode-eslint",
    "esbenp.prettier-vscode",
    "github.vscode-pull-request-github",
    "silvenon.mdx"
  ],
  "postCreateCommand": "yarn install",
  "containerEnv": {
    "GATSBY_TELEMETRY_DISABLED": "1"
  },
  "remoteUser": "node"
}
